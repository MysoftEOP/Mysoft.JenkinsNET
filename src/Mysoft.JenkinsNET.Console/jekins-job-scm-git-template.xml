<?xml version="1.0" encoding="utf-8" ?>
<project>
  <description/>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.dabsquared.gitlabjenkins.connection.GitLabConnectionProperty plugin="gitlab-plugin@1.5.4">
      <gitLabConnection/>
    </com.dabsquared.gitlabjenkins.connection.GitLabConnectionProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.TextParameterDefinition>
          <name>RDC_CODE_REPOSITORY</name>
          <description>当前的构建分支的git仓库地址</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>RDC_BRANCH_NAME</name>
          <description>当前的构建分支名</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>RDC_DB_INSTANCE</name>
          <description>当前构建环境所使用的数据库实例名</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>RDC_DB_DATABASE</name>
          <description>当前构建环境所使用的数据库名</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>RDC_DB_UNAME</name>
          <description>当前构建环境所使用的数据库用户名</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>RDC_DB_PWD</name>
          <description>当前构建环境所使用的数据库密码</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>RDC_METADATA_PATH</name>
          <description>当前构建环境元数据目录，在构建过程中会调用元数据同步工具来执行数据同步</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>RDC_BASE_JOB_NAME</name>
          <description>多个job之间共享的workspace目录</description>
          <defaultValue></defaultValue>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>RDC_PORT</name>
          <description>docker port</description>
          <defaultValue></defaultValue>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>TAG</name>
          <description>版本标签</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>AppNameKey</name>
          <description>AppNameKey</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>PackageDir</name>
          <description>PackageDir</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>BranchConfig</name>
          <description>BranchConfig</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>DeploymentTaskId</name>
          <description>部署任务Id</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>DockerImageName</name>
          <description>docker镜像名称</description>
          <defaultValue/>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  #{SCM}
  <assignedNode>#{Node}</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>${WORKSPACE_ROOT}\${RDC_BASE_JOB_NAME}</customWorkspace>
  <builders>#{BatchCmd}</builders>
  <publishers/>
  <buildWrappers/>
</project>